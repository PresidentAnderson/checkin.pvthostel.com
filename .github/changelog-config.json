{
  "categories": [
    {
      "title": "## ğŸš€ Features",
      "labels": ["feature", "enhancement"]
    },
    {
      "title": "## ğŸ› Bug Fixes",
      "labels": ["bug", "fix"]
    },
    {
      "title": "## ğŸ”’ Security",
      "labels": ["security"]
    },
    {
      "title": "## ğŸ“¦ Dependencies",
      "labels": ["dependencies"]
    },
    {
      "title": "## ğŸ“š Documentation",
      "labels": ["documentation"]
    },
    {
      "title": "## ğŸ§° Maintenance",
      "labels": ["chore", "maintenance"]
    }
  ],
  "ignore_labels": [
    "ignore-for-release"
  ],
  "sort": {
    "order": "ASC",
    "on_property": "mergedAt"
  },
  "template": "${{CHANGELOG}}\n\n**Full Changelog**: ${{OWNER}}/${{REPO}}/compare/${{PREVIOUS_TAG}}...${{RELEASE_TAG}}",
  "pr_template": "- ${{TITLE}} by @${{AUTHOR}} in #${{NUMBER}}",
  "empty_template": "No changes",
  "label_extractor": [
    {
      "pattern": "(feat|feature)",
      "target": "feature"
    },
    {
      "pattern": "fix",
      "target": "bug"
    },
    {
      "pattern": "(chore|build|ci)",
      "target": "chore"
    }
  ],
  "duplicate_filter": {
    "pattern": ".*",
    "on_property": "title",
    "method": "match"
  },
  "transformers": [
    {
      "pattern": "[\\-\\*] (\\[(...|TEST)\\])( )?(.+?)\n",
      "target": "- $4\n"
    }
  ],
  "max_tags_to_fetch": 200,
  "max_pull_requests": 200,
  "max_back_track_time_days": 365,
  "exclude_merge_branches": [
    "main"
  ],
  "tag_resolver": {
    "method": "semver",
    "filter": {
      "pattern": "v(.+)",
      "flags": "gu"
    }
  },
  "base_branches": [
    "main"
  ]
}